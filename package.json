{
    "name": "metalsmith-babylast",
    "fullname": "Metalsmith Babylast",
    "description": "Add a Babylon AST to each Metalsmith file object.",
    "version": "0.1.0",
    "keywords": [ "metalsmith", "babylon", "ast", "javascript" ],
    "main": "./lib/index.js",
    "dependencies": {
        "babylon":    "^6.3",
        "lodash":     "^3.0"
    },
    "devDependencies": {
        "metalsmith": "^2.1",
        "mocha":      "^2.0",
        "should":     "^7.0",
        "del":        "^2.1",
        "gulp":       "^3.9",
        "gulp-ejs":   "^1.2"
    },
    "scripts": {
        "build":   "./node_modules/.bin/gulp",
        "clobber": "./node_modules/.bin/gulp clobber",
        "test":    "./node_modules/.bin/mocha --harmony -R spec $(find ./test -n '*.js')"
    },
    "license": "MIT",
